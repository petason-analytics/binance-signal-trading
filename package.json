{
  "name": "launchpad",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:launchpad": "node dist/apps/launchpad/main",
    "start:admin": "node dist/apps/admin/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/launchpad/test/jest-e2e.json",
    "generate": "rm -rf libs/prisma/src/@generated/prisma-nestjs-graphql; npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "db:push": "npx prisma db push"
  },
  "dependencies": {
    "@anchan828/nest-sendgrid": "^0.5.32",
    "@casl/ability": "^5.4.3",
    "@metamask/eth-sig-util": "^4.0.0",
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/throttler": "^2.0.0",
    "@prisma/client": "^3.11.0",
    "@sentry/node": "^6.17.9",
    "@sentry/tracing": "^6.17.9",
    "apollo-server-express": "^3.6.3",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.0.2",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ethers": "^5.5.4",
    "graphql": "^16.3.0",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-subscriptions": "^2.0.0",
    "ioredis": "^4.28.5",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.3.1",
    "nestjs-log": "^2.0.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "password-validator": "^5.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql-type-json": "^0.3.2",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^3.11.0",
    "prisma-graphql-type-decimal": "^1.0.0",
    "prisma-nestjs-graphql": "^14.6.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@lib/helper(|/.*)$": "<rootDir>/libs/helper/src/$1",
      "^@lib/acl(|/.*)$": "<rootDir>/libs/acl/src/$1",
      "^@lib/blockchain(|/.*)$": "<rootDir>/libs/blockchain/src/$1"
    }
  }
}
