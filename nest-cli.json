{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/trade/src",
  "monorepo": true,
  "root": "apps/trade",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/trade/tsconfig.app.json"
  },
  "projects": {
    "trade": {
      "type": "application",
      "root": "apps/trade",
      "entryFile": "main",
      "sourceRoot": "apps/trade/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trade/tsconfig.app.json"
      }
    },
    "launchpad": {
      "type": "application",
      "root": "apps/launchpad",
      "entryFile": "main",
      "sourceRoot": "apps/launchpad/src",
      "compilerOptions": {
        "tsConfigPath": "apps/launchpad/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
